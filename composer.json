{
    "name": "lanin/laravel-setup-wizard",
    "description": "Provide your Laravel project with handy setup wizard",
    "keywords": ["laravel", "framework", "installer", "setup", "wizard"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Lanin",
            "email": "max@lanin.me",
            "homepage": "http://lanin.me"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.1",
        "illuminate/console": "~5.1",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "^4.7.6",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
           "Lanin\\Laravel\\SetupWizard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lanin\\Laravel\\SetupWizard\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
